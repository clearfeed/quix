generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model SlackWorkspace {
  @@map("slack_workspaces")
  id String @id @default(uuid())
  name String
  team_id String
  bot_access_token String
  authed_user_id String
  bot_user_id String
  is_enterprise_install Boolean
  scopes String[]
}
