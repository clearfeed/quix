{
  "name": "openai-hubspot-integration",
  "version": "1.0.0",
  "description": "OpenAI function calling integration with HubSpot",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "link-packages": "yarn link @clearfeed-agent/hubspot && yarn link @clearfeed-agent/jira && yarn link @clearfeed-agent/github",
    "postinstall": "cd agent-packages && yarn install && yarn build && cd .. && yarn run link-packages"
  },
  "keywords": [
    "openai",
    "hubspot",
    "function-calling",
    "typescript",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@clearfeed-agent/github": "file:agent-packages/packages/github",
    "@clearfeed-agent/hubspot": "file:agent-packages/packages/hubspot",
    "@clearfeed-agent/jira": "file:agent-packages/packages/jira",
    "@slack/web-api": "^7.8.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "openai": "^4.83.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "eslint": "^9.19.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node-dev": "^2.0.0"
  }
}
