{
  "name": "@clearfeed-ai/quix-bamboohr-agent",
  "version": "2.2.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "tsc --build --clean",
    "build": "yarn clean && tsc",
    "test": "jest",
    "test:integration": "jest --testNamePattern='Integration'",
    "watch": "tsc --watch",
    "prepublishOnly": "yarn build"
  },
  "peerDependencies": {
    "@langchain/core": "1.1.8"
  },
  "dependencies": {
    "@clearfeed-ai/quix-common-agent": "2.1.1",
    "axios": "^1.13.5"
  },
  "devDependencies": {
    "typescript": "^5.7.3",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "@types/node": "^22.10.7",
    "dotenv": "^16.4.5"
  },
  "license": "Apache-2.0",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.(t|j)s"
    ],
    "coverageDirectory": "coverage",
    "testTimeout": 30000,
    "setupFilesAfterEnv": [
      "<rootDir>/src/test-setup.ts"
    ]
  }
}
