{
  "name": "@clearfeed-ai/quix-notion-agent",
  "version": "2.2.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "tsc --build --clean",
    "build": "yarn clean && tsc",
    "test": "jest",
    "prepublishOnly": "yarn build",
    "watch": "tsc --watch"
  },
  "peerDependencies": {
    "@langchain/core": "1.1.8"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": "src/.*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.spec.json"
        }
      ]
    },
    "collectCoverageFrom": [
      "src/**/*.(t|j)s"
    ],
    "coverageDirectory": "coverage"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.8.3"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@clearfeed-ai/quix-common-agent": "2.1.0",
    "@notionhq/client": "^3.0.1",
    "lodash": "^4.17.21"
  }
}
